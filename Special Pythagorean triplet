#include <bits/stdc++.h>
using namespace std;
#define llu long long unsigned
int main()
{
	ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    llu ans,flag=0;
	for(llu i=1;i<=1000;i++)
	{
		for(llu j=i+1;j<=1000;j++)
		{
			for(llu k=j+1;k<=1000;k++)
			{
				llu h=pow(k,2),b=pow(j,2),p=pow(i,2);
				if(h==b+p&&i+j+k==1000)
				{
	             cout<<i*j*k;flag=1;
				}
			}
	
		}
	
	}
    return 0;
}
